{
  "compileOnSave": false,
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "removeComments": false,
    "target": "esnext",
    "moduleResolution": "Node",
    "types": [
      "jquery",
      "tampermonkey",
      "@userscripters/stackexchange-global-types",
      "format-unicorn",
      "js-beautify"
    ]
  },
  "include": [
    "./**/*"
  ]
}